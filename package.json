    "image-type": "^5.2.0",
    "ipfs-core-types": "^0.14.0",
    "it-all": "^2.0.0",
    "jest": "^29.4.1",
    "keyv": "^4.5.2",
    "libp2p": "^0.42.2",
    "os-browserify": "^0.3.0",
