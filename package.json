{
  "version": "0.6.37",
  "license": "AGPL-3.0-or-later",
  "name": "@constl/ipa",
  "author": "Julien Jean Malard-Adam",
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "exports": "./dist/src/index.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "nettoyer": "rimraf dist",
    "compiler": "pnpm versioner && pnpm nettoyer && pnpm ttsc -p tsconfig.build.json",
    "test": "node --no-warnings --experimental-vm-modules ./node_modules/jest/bin/jest.js --runInBand",
    "test:proc": "env MANDATAIRE=PROC pnpm test",
    "test:tous": "env MANDATAIRE=TOUS pnpm test",
    "test:aegir": "pnpm compiler && env MANDATAIRE=TOUS pnpm aegir test && pnpm aegir test -t browser",
    "couverture": "pnpm test -- --coverage",
    "couverture:tous": "pnpm test:tous -- --coverage --bail",
    "formatter": "pnpm prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\" && pnpm eslint --ext .js,.ts src",
    "publier": "pnpm versioner && pnpm compiler && (git commit -a -m 'version' || true) && pnpm publish",
    "versioner": "pnpm genversion  --es6 --semi src/version.ts"
  },
  "watch": {
    "build": {
      "patterns": "./src/**/*",
      "extensions": [
        ".ts",
        ".tsx"
      ]
    },
    "postbuild": "./dist/**/*"
  },
  "devDependencies": {
    "@constl/orbit-db-types": "^1.0.7",
    "@jest/globals": "^29.4.2",
    "@libp2p/interface-connection-manager": "^1.3.7",
